<p id="notice"><%= notice %></p>

<h1>Listing Dr Planneds</h1>

<table>
  <thead>
    <tr>
      <th>Prosumer</th>
      <th>Volume</th>
      <th>Timestamp</th>
      <th>Demand response</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dr_planneds.each do |dr_planned| %>
      <tr>
        <td><%= dr_planned.prosumer %></td>
        <td><%= dr_planned.volume %></td>
        <td><%= dr_planned.timestamp %></td>
        <td><%= dr_planned.demand_response %></td>
        <td><%= link_to 'Show', dr_planned %></td>
        <td><%= link_to 'Edit', edit_dr_planned_path(dr_planned) %></td>
        <td><%= link_to 'Destroy', dr_planned, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dr planned', new_dr_planned_path %>
