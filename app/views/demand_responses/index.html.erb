<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        Listing demand response signals
      </div>
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped table-bordered table-hover" id="dataTables-data-points">
            <thead>
            <tr>
              <th><%= "starttime"-%></th>
              <th><%= "starttime" -%></th>
              <th><%= "interval" -%></th>
              <th colspan="2"></th>
            </tr>
            </thead>

            <tbody>
            <% @demand_responses.each do |demand_response| %>
                <tr>
                  <td><%= demand_response.starttime %></td>
                  <td><%= demand_response.stoptime %></td>
                  <td><%= demand_response.interval.duration %></td>
                  <td><%= link_to 'Show', demand_response, class: 'btn btn-success btn-xs' %></td>
                  <td><%= link_to 'Destroy', demand_response, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger btn-xs' %></td>
                </tr>
            <% end %>
            </tbody>
          </table>
          <%= will_paginate @demand_responses, renderer: BootstrapPagination::Rails %>
        </div>
        <!-- /.table-responsive -->
        <%= link_to 'New Demand Response', new_demand_response_path, class: 'btn btn-default btn-xs' %>
      </div>
    </div>
  </div>
</div>
