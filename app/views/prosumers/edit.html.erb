<div class="row">
  <div class="col-lg-12">
    <h3 class="page-header"><small>Editing prosumer:</small> <%= @prosumer.name -%>   
      <small> 
        <%= link_to 'Show', @prosumer,class:'btn btn-success btn-xs' %> 
        | <%= link_to 'All prosumers', prosumers_path,class:'btn btn-warning btn-xs' %>
        | <%= link_to @prosumer.prosumer_category.name, prosumers_path(category: @prosumer.prosumer_category) ,class: 'btn btn-info btn-xs' if @prosumer.prosumer_category %>
      </small>
    </h3>
  </div>
  <!-- /.col-lg-12 -->
</div>

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        Prosumer properties
      </div>
      <div class="panel-body">
        <%= render 'form' %>
       </div>
    </div>
  </div>
  <!-- /.col-lg-12 -->
</div>       



